{
  "public": ".",
  "rewrites": [
    { "source": "/login", "destination": "/login.html" },
    { "source": "/index", "destination": "/index.html" },
    { "source": "/**", "destination": "/index.html" }
  ],
  "cleanUrls": false,
  "headers": [
    {
      "source": "**/*.@(js|css|jpg|jpeg|png|gif|ico|svg|woff|woff2|ttf|eot)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ]
}
